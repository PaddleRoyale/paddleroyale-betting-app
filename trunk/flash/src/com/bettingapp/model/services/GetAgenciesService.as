package com.bettingapp.model.services{	import com.adobe.serialization.json.JSON;		import com.bettingapp.controller.events.BettingAppEvent;	import com.bettingapp.vo.Agency;	import com.bettingapp.vo.WebServiceResults;		import flash.events.Event;	import flash.net.URLRequest;	import flash.net.URLRequestMethod;		public class GetAgenciesService extends WebServiceBase	{		public function GetAgenciesService()		{			super();						// setup parameters exclusive to this command.			_urlRequest.method = URLRequestMethod.GET;			_urlRequest.url = BASE_URL + "betting/GetAgencies";		}				protected override function onResult($e:Event):void		{			trace(_urlLoader.data);						// Flash 11 commented out as other developers are not on cs6 yet.			//var result:Object = JSON.parse(_urlLoader.data);			var result:Object = com.adobe.serialization.json.JSON.decode(_urlLoader.data);						var agencies:Array = new Array();						for each (var agency:Object in result.Agencies)			{				var newAgency:Agency = new Agency();				newAgency.id = int(agency.id);				newAgency.name = String(agency.name);				agencies.push(newAgency);			}						var event:BettingAppEvent = new BettingAppEvent(BettingAppEvent.WEB_SERVICE_RESULT);			event.parameters.result = WebServiceResults.RESULT_SUCCESS;			event.parameters.agencies = agencies;			_eventManager.dispatchEvent(event);							removeListeners();				// straight override, no calling base class.		}	}}