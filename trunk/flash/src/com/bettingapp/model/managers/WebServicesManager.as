package com.bettingapp.model.managers{	import com.bettingapp.model.services.*;	import com.bettingapp.vo.Bet;		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.events.IEventDispatcher;		public class WebServicesManager extends EventDispatcher	{		private static var _instance:WebServicesManager;		private var _getVoucherService:GetVoucherService;		private var _placeBetService:PlaceBetService;		private var _getAgenciesService:GetAgenciesService;		private var _getPlayersService:GetPlayersService;		private var _getPlayerNextMatchService:GetPlayerNextMatchService;		private var _addCreditsService:AddCreditsService;		private var _processBetsService:ProcessBetsService;		private var _getWinningBetTotalsService:GetWinningBetTotalsService;				private var _activeBet:Bet = new Bet();				public function WebServicesManager($singleton:SingletonEnforcer)		{			super();		}				public static function get instance():WebServicesManager		{			if (WebServicesManager._instance == null)			{				WebServicesManager._instance = new WebServicesManager(new SingletonEnforcer);			}			return WebServicesManager._instance;		}				public function GetVoucher($id:int):void		{			trace("GetVoucher");			_getVoucherService = new GetVoucherService();			_getVoucherService.urlVariables.id = $id;			_getVoucherService.execute();		}				public function PlaceBet():void		{			trace("PlaceBet");			_placeBetService = new PlaceBetService();			_placeBetService.urlVariables.id = _activeBet.id;			_placeBetService.urlVariables.type = _activeBet.type;			_placeBetService.urlVariables.value = _activeBet.value;			_placeBetService.execute();		}				public function GetAgencies():void		{			trace("GetAgencies");			_getAgenciesService = new GetAgenciesService();			_getAgenciesService.execute();		}				public function GetPlayers():void		{			_getPlayersService = new GetPlayersService();			_getPlayersService.execute();		}				public function GetPlayerNextMatch($playerId:int)		{			_getPlayerNextMatchService = new GetPlayerNextMatchService();			_getPlayerNextMatchService.urlVariables.playerId = $playerId; 			_getPlayerNextMatchService.execute();		}				public function AddCredits($id:int, $numCreditsToAdd:uint) 		{			_addCreditsService = new AddCreditsService();			_addCreditsService.urlVariables.id = $id;			_addCreditsService.urlVariables.creditsToAdd = $numCreditsToAdd;			_addCreditsService.execute();		}				public function ProcessBets() 		{			_processBetsService = new ProcessBetsService();			_processBetsService.execute();		}				public function GetWinningBetTotals()		{			_getWinningBetTotalsService = new GetWinningBetTotalsService();			_getWinningBetTotalsService.execute();		}		public function get activeBet():Bet		{			return _activeBet;		}	}}internal class SingletonEnforcer {}